  },
  "homepage": "https://github.com/eirenelakshita/kdev#readme",
  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "if-env": "^1.0.4",
    "moment": "^2.22.2",
    "mongoose": "^5.0.17",
    "net": "^1.0.2",
    "nodemailer": "^4.6.8",
    "passport": "^0.4.0",
